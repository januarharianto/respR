@article{legrandReducedPhysiologicalPerformance2021,
  title = {Reduced Physiological Performance in a Free-Living Coralline Alga Induced by Salmon Faeces Deposition},
  author = {Legrand, E and Kutti, T and Gonzalez Casal, Ev and Rastrick, Sps and Andersen, S and Husa, V},
